{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "blocktheme/card-project",
    "title": "Card Project",
    "category": "theme",
    "icon": "portfolio",
    "description": "Carte de projet avec titre, description, bouton et image",
    "supports": {
        "html": false,
        "anchor": true
    },
    "attributes": {
        "title": {
            "type": "string",
            "default": ""
        },
        "content": {
            "type": "string",
            "default": ""
        },
        "buttonText": {
            "type": "string",
            "default": ""
        },
        "buttonUrl": {
            "type": "string",
            "default": "#"
        },
        "imageUrl": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "blocktheme",
    "editorScript": "file:../../../build/index.js",
    "style": "file:./card-project.css",
    "render": "file:./card-project.php"
}