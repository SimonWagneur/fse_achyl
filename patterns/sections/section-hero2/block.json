{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "blocktheme/section-hero2",
    "title": "Section Hero 2",
    "category": "sections",
    "icon": "align-wide",
    "description": "Section hero avec titre, texte et boutons",
    "supports": {
        "html": false,
        "anchor": true
    },
    "attributes": {
        "title": {
            "type": "string",
            "default": ""
        },
        "text": {
            "type": "string",
            "default": ""
        },
        "imageUrl": {
            "type": "string",
            "default": ""
        },
        "imageId": {
            "type": "number",
            "default": 0
        },
        "imageAlt": {
            "type": "string",
            "default": ""
        }
    },
    "editorScript": "file:../../../build/index.js",
    "render": "file:./section-hero2.php",
    "style": "file:./section-hero2.css"
}